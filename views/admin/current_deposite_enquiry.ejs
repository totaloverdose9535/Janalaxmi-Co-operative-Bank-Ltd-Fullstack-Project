<%- include('navbar.ejs') %>



<div class="container mt-5 mb-3">
    <h2>Export and Print Page</h2>
    <button class="btn btn-primary" onclick="copyText()">Copy Text</button>
    <button class="btn btn-success" onclick="downloadCSV()">Download CSV</button>
    <button class="btn btn-info" onclick="downloadExcel()">Download Excel</button>
    <button class="btn btn-danger" onclick="downloadPDF()">Download PDF</button>
    <button class="btn btn-warning" onclick="printPage()">Print Page</button>
</div>

<div id="content" class="container-fluid bg-white p-4">
    <div class="row">
        <div class="col-md-12">
            <h3 style="text-align: center;">Current Deposit Scheme Enquiry</h3>
            <br>
        </div>
        <div class="col-md-12">
            <table class="table table-sm table-bordered">
                <tr>
                    <th>Action</th>
                    <th>SN</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Mobile No</th>
                    <th>City</th>
                    <th>Message</th>

                </tr>
                <% for(i=0;i<current_deposit_enquiry.length;i++) { %>

                    <tr>
                        <td style="white-space: nowrap;">
                            <a href="/admin/delete_current_deposit_enquiry/<%- current_deposit_enquiry[i].current_deposit_enquiry_id %>">
                                <button class="btn btn-sm btn-danger"><i class="fa fa-trash"></i></button>
                            </a>  
                        </td>
                        <td><%-i+1%></td>
                        <td><%-current_deposit_enquiry[i].current_enquiry_name%></td>
                        <td><%-current_deposit_enquiry[i].current_enquiry_email%></td>
                        <td><%-current_deposit_enquiry[i].current_enquiry_mobile%></td>
                        <td><%-current_deposit_enquiry[i].current_enquiry_city%></td>
                        <td><%-current_deposit_enquiry[i].current_enquiry_message%></td>

                    </tr>
                <% } %>
            </table>
        </div>
    </div>
</div>


<%- include('footer.ejs') %>

