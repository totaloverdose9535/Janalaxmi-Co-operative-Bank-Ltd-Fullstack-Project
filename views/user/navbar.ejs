    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-xl" style="max-width: 90%;">
            <div class="modal-content">
                <div class="modal-header" style="background-color: rgb(88, 88, 255);">
                    <h5 class="modal-title text-white" id="exampleModalLabel">Customer Support Form</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>

                <div class="modal-body">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-6">
                                <img class="csimg w-100 rounded-1" src="https://images.pexels.com/photos/8867429/pexels-photo-8867429.jpeg?auhref=compress&cs=tinysrgb&w=1260&h=750&dpr=2" alt="" style="height: 400px;" />
                                <div class="mt-3">
                                    <h6>
                                        <a href="mailto:<%-company_info.company_email%>" class="text-decoration-none">
                                            <i class="fa-solid fa-envelope"></i>
                                            <%-company_info.company_email%>
                                        </a>
                                        <a href="tel:+91<%-company_info.company_mobile%>" class="ml-5 text-decoration-none">
                                            <i class="fa-solid fa-phone"></i> +91
                                            <%-company_info.company_mobile%>
                                        </a>
                                    </h6>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <form action="/customer_support_data" method="post" enctype="multipart/form-data">
                                    <div class="">
                                        <label for="customer_name" class="form-label fw-bold">NAME (नाव) <span class="text-danger"> *</span></label>
                                        <input type="text" class="form-control" id="customer_name" name="customer_name" required placeholder="Enter Name" />
                                    </div>

                                    <div class="">
                                        <label for="customer_email" class="form-label fw-bold">E-Mail (ई-मेल) <span class="text-danger"> *</span></label>
                                        <input type="email" class="form-control" id="customer_email" name="customer_email" required placeholder="E-Mail Address" />
                                    </div>

                                    <div class="">
                                        <label for="customer_mobile" class="form-label fw-bold">CONTACT NO (मोबाईल क्र.) <span class="text-danger"> *</span></label>
                                        <div class="input-group">
                                            <span class="input-group-text">+91</span>
                                            <input type="tel" maxlength="10" pattern="\d{10}" oninput="this.value = this.value.replace(/\D/g,'')" inputmode="numeric" class="form-control" id="customer_mobile" name="customer_mobile" required placeholder="Enter your mobile number" />
                                        </div>
                                    </div>


                                    <div class="">
                                        <label for="customer_address" class="form-label fw-bold">Address ( पत्ता ) <span class="text-danger">*</span></label>
                                        <input type="text" class="form-control" id="customer_address" name="customer_address" placeholder="Address" />
                                    </div>

                                    <div class="">
                                        <label for="customer_query" class="form-label fw-bold">ENTER YOUR MESSAGE (मेसेज) <span class="text-danger">*</span></label>
                                        <textarea class="form-control" id="customer_query" name="customer_query" placeholder="Leave a comment here" rows="4" required></textarea>
                                    </div>

                                    <div class="d-flex justify-content-between">
                                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer"></div>
            </div>
        </div>
    </div>


