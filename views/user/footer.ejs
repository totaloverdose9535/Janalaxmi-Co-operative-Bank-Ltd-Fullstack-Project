<!-- auto time and date function for enquiry form start -->
<script>
    function updateDateTime() {
        const currentDate = new Date();
        document.getElementById('currentDate').value = currentDate.toLocaleDateString();
        document.getElementById('currentTime').value = currentDate.toLocaleTimeString();
    }

    // Update date and time every second
    setInterval(updateDateTime, 1000);

    // Initialize on page load
    window.onload = updateDateTime;
</script>

<!-- auto time and date function for enquiry form end -->


