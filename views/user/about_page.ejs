<%- include("navbar.ejs") %>
    <div class="formoving">


        <section class=" pt-5 pb-5 ">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-12  d-md-inline mt-3 ab_or">
                        <img src="/uploads/<%-about.about_image%>" class="aboutimg img-fluid">
                    </div>
                    <div class="col-lg-6 col-md-12 aboutinfo">
                        <h2 class=" font-weight-bold mt-2" data-aos="fade-up" data-aos-duration="1000">ABOUT ORGANIZATION</h2>
                        <br><br>
                        <h5 class=" mt-3 font-weight-bold">
                            <%-about.about_heading%>
                        </h5>
                        <p class=" mt-3   ">
                            <%-about.about_description%>
                        </p>
                    </div>
                </div>
            </div>
        </section>

        <section>
            <div class="container mt-5">
                <h1 class="text-center" style="color: #01257D;" data-aos="fade-up" data-aos-duration="1000">Our Branches</h1>
                <span class="section-heading-border border-center mb-3 text-center"></span> <br>
                <div class="row">
                    <% for (var i=0;i<branch.length;i++) { %>
                        <div class="col-lg-4 col-sm-6 col-12 mb-3" data-aos="fade-up" data-aos-duration="1500">

                            <div class="aboutcard custom-card text-center" style="box-shadow:0.5 ;">
                                <div class="card-body py-2">
                                    <span class="h3 mt-3"><%-branch[i].branch_city%></span><br>
                                    <h5 class="card-title mt-2">
                                        <%-branch[i].branch_header%>
                                    </h5>
                                    <p class="card-text mb-3">
                                        <%-branch[i].branch_lane%>,
                                            <%-branch[i].branch_lane2%> <br> Dist. Ahemdnagar<br>
                                                <i class="fa-solid fa-phone"></i>
                                                <%-branch[i].branch_no%>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <% } %>

                </div>
            </div>
        </section>


        <section>
            <div class="container pt-5 py-5">
                <h1 class="text-center" style="color: #01257D;" data-aos="fade-up" data-aos-duration="1000">Our Vision</h1><br>
                <span class="section-heading-border border-center mb-3 text-center"></span>
                <div class="row">
                    <div class="col-lg-6 col-12 text-center">
                        <p class=" vis  text-center vision_text">
                            <%-vision.our_vision_description%>
                        </p>
                        <!-- <a href="vision.html" class="btn btn-primary text-center mt-5">Read More</a> -->
                    </div>
                    <div class="col-lg-6 col-12">
                        <img src="/uploads/<%-vision.our_vision_image%>" class="visionimg"></img>
                    </div>
                </div>
            </div>
        </section>

        <section style="background-color:rgb(73,73,200)"><br><br>
            <!-- <div class="container-fluid bg-info">
    <div class="row">
       <div class="col-12"> -->


            <div class="container-fluid text-white history">
                <div class="row">
                    <div class="col-12">
                        <div class="container">


                            <h1 class="text-center" data-aos="fade-right" data-aos-duration="1500">Our History<img src="/uploads/<%-history.our_history_image%>" class="ml-5"></h1>
                            <span class="section-heading-border border-center mb-3 text-center"></span>
                            <div class="row">
                                <div class="py-2">
                                    <p class="history_para mb-5">
                                        <%-history.our_history_description%>
                                    </p>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- </div>
</div>
</div> -->
        </section>

        <section class="mt-5 mb-5">
            <div class="container pt-5">
                <h1 class="text-center" style="color: #01257D;" data-aos="fade-up" data-aos-duration="1500">Our mission</h1><br>
                <span class="section-heading-border border-center"></span>
                <div class="row">
                    <div class="col-lg-6 col-sm-12 col-12 text-center">
                        <p class="mt-5 mis">
                            <%-mission.our_mission_description%>
                        </p>
                        <!-- <a href="mission.html" class="btn btn-primary text-center">Read More</a> -->
                    </div>
                    <div class="col-lg-6 col-sm-12 col-12">
                        <img src="/uploads/<%-mission.our_mission_image%>" class="missionimg"></img>
                    </div>
                </div>
            </div>
        </section>

        <section class="container mt-5  touch">
            <h1 class="text-center" style="color: #01257D;" data-aos="fade-up" data-aos-duration="1000">Get In Touch</h1><br>
            <span class="section-heading-border border-center mb-3"></span>
            <div class="container mt-2 touchcard1">
                <div class="row">
                    <div class="col-lg-3 col-12 ">
                        <div class="touchcard py-3">
                            <i class="fa-solid fa-briefcase mb-4" data-aos="fade-up" data-aos-duration="1500"></i>
                            <div class="card-body mb-3 touchmed">
                                <h4 class="card-title mb-1" data-aos="fade-up" data-aos-duration="1000">Head Office</h4>
                                <p class="card-text p-3">
                                    <%-get_touch.head_office_address%>
                                </p>

                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-12 mt-4">
                        <div class="touchcard py-3">
                            <i class="fa-solid fa-phone mb-4" data-aos="fade-up" data-aos-duration="1500"></i>
                            <div class="card-body mb-3">
                                <h4 class="card-title mb-1" data-aos="fade-up" data-aos-duration="1000">Call us</h4>
                                <p class="card-text p-3">
                                    <%-get_touch.contact_no%>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-12 mt-4">
                        <div class="touchcard py-3">
                            <i class="fa-solid fa-envelope mb-4" data-aos="fade-up" data-aos-duration="1500"></i>
                            <div class="card-body mb-3">
                                <h4 class="card-title mb-1" data-aos="fade-up" data-aos-duration="1000">Email Address</h4>
                                <p class="card-text p-3">
                                    <%-get_touch.email%>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-12 mt-4">
                        <div class="touchcard py-3">
                            <i class="fa-solid fa-clock mb-4" data-aos="fade-up" data-aos-duration="1500"></i>
                            <div class="card-body mb-3">
                                <h4 class="card-title mb-1" data-aos="fade-up" data-aos-duration="1000">Timings</h4>
                                <p class="card-text p-3">
                                    <%-get_touch.time%>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
