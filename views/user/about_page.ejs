        <section class="mt-5">
            <h1 class="text-center" style="color: #01257D;" data-aos="fade-up" data-aos-duration="1000">Happy Customer Review's</h1><br>
            <span class="section-heading-border border-center mb-3"></span>
            <br>
            <div class="customer">
                <div class="row">
                    <div class="col-12 happy_customer  customer1 " style="height: 400px;">
                        <div id="carouselExample" class="carousel slide h-100 w-100 ">
                            <div class="carousel-inner mt-4">
                                <div class="carousel-item active">
                                    <img src="/uploads/<%-review[0].customer_image%>" class="text-center rounded-circle rounded mx-auto d-block mt-5" style="border-radius: 50%; height: 100px;width: 100px;margin-top: 20px;" />
                                    <p class="d-block fs-5 w-100 text-center text-white mt-2">
                                        <%-review[0].about_description%>
                                    </p>
                                    <h3 class="d-block w-100 text-center text-white mt-2">
                                        <%-review[0].customer_name%>
                                    </h3>
                                    <h6 class="d-block w-100 text-center text-white mt-2 mb-5">
                                        <%-review[0].customer_position%>
                                    </h6>

                                </div>
                                <% for (var i=1;i<review.length;i++) { %>

                                    <div class="carousel-item">
                                        <img src="/uploads/<%-review[i].customer_image%>" class="text-center rounded-circle rounded mx-auto d-block mt-5" style="border-radius: 50%; height: 100px;width: 100px;" />
                                        <p class="d-block fs-5 w-100 text-center text-white mt-2">
                                            <%-review[i].about_description%>
                                        </p>
                                        <h3 class="d-block w-100 text-center text-white mt-2">
                                            <%-review[i].customer_name%>
                                        </h3>
                                        <h6 class="d-block w-100 text-center text-white mt-2 mb-5">
                                            <%-review[i].customer_position%>
                                        </h6>
                                    </div>

                                    <% } %>

                            </div>
                            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
                              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                              <span class="visually-hidden">Previous</span>
                            </button>
                            <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
                              <span class="carousel-control-next-icon" aria-hidden="true"></span>
                              <span class="visually-hidden">Next</span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <%- include("footer.ejs") %>