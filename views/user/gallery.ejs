<%- include ("navbar.ejs") %>

    <div class="formoving">

        <div class="container-fluid p-0">
            <div class="row">
                <div class="col-12 image-container">
                    <div class="image-wrapper">
                        <img src="/uploads/<%= gallery_bg.gallery_bg_image %>" alt="Image" id="img_opcity">
                    </div>
                    <div class="text-content container mt-5">
                        <h3 class="text-center text-primary mt-4">Gallery</h3>
                        <div class="row mt-4">

                            <% for(var i=0; i<gallery_image.length; i++) { %>
                                <div class="col-12 col-md-4 gallimg text-center mt-3 text-md-start h-100">
                                    <img src="/uploads/<%= gallery_image[i].gallery_image %>" class="h-75 gallery-image" style="width: 300px; " data-bs-toggle="modal" data-bs-target="#imageModal">
                                    <p class="text-center mb-5">Nagpur City Multistate Co-operative Credit Society was presented with the Arjun Kapoor. Wasnik!
                                    </p>
                                </div>
                                <% } %>

                                    <% for(var i=0; i<gallery_video.length; i++) { %>
                                        <div class="col-12 col-md-6 text-center text-md-start mb-3 ">
                                            <iframe src="/uploads/<%= gallery_video[i].gallery_video %>" title="video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin"
                                                allowfullscreen class="w-75 " height="250px"></iframe>
                                            <p class="text-center mt-3 mb-3 w-75">Mr. Jitendra Wasnik, He interacted with Lokmanch channel, encouraging students and wishing them success!!</p>
                                        </div>
                                        <% } %>


                                            <div class="col-md-12 mt-5">
                                                <nav aria-label="Page navigation example">
                                                    <ul class="pagination justify-content-center">
                                                        <li class="page-item">
                                                            <a class="page-link" href="#" aria-label="Previous">
                                                                <span aria-hidden="true">&laquo;</span>
                                                                <span class="sr-only">Previous</span>
                                                            </a>
                                                        </li>
                                                        <% for(var i=1; i<=total_pages; i++) { %>

                                                            <li class="page-item">
                                                                <a class="page-link" href="/gallery?page_no=<%=i%>">
                                                                    <%= i %>
                                                                </a>
                                                            </li>
                                                            <% } %>

                                                                <li class="page-item">
                                                                    <a class="page-link" href="#" aria-label="Next">
                                                                        <span aria-hidden="true">&raquo;</span>
                                                                        <span class="sr-only">Next</span>
                                                                    </a>
                                                                </li>
                                                    </ul>
                                                </nav>
                                            </div>

                        </div>

                    </div>

                </div>

            </div>
        </div>

    </div>
    <br>
    <br><br>



    <%- include ("footer.ejs") %>